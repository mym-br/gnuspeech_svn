<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>

	<!-- ========== Program parameters ========== -->

	<key>ArticulationData</key>
	<string>diphones</string>

	<!-- ========== Controller parameters ========== -->

	<key>ShouldUseMacroIntonation</key>
	<true/>

	<key>ShouldUseMicroIntonation</key>
	<true/>

	<key>ShouldUseDrift</key>
	<true/>

	<key>ShouldUseSmoothIntonation</key>
	<true/>

	<key>DriftGeneratorDeviation</key>
	<real>1.0</real>

	<key>DriftGeneratorSampleRate</key>
	<real>500.0</real>

	<key>DriftGeneratorLowpassCutoff</key>
	<real>4.0</real>

	<key>RadiusMultiply</key>
	<real>1.0</real>

	<key>IntonationNotionalPitch</key>
	<real>-1.0</real>

	<key>IntonationPretonicRange</key>
	<real>2.0</real>

	<key>IntonationPretonicLift</key>
	<real>-2.0</real>

	<key>IntonationTonicRange</key>
	<real>-10.0</real>

	<key>IntonationTonicMovement</key>
	<real>-6.0</real>

	<key>GlobalTempo</key>
	<real>1.0</real>

	<!-- ========== Synthesis parameters ========== -->

	<key>MasterVolume</key>
	<real>60.0</real>

	<key>VocalTractLength</key>
	<real>17.5</real>

	<key>Temperature</key>
	<real>25.0</real>

	<key>Balance</key>
	<real>0.0</real>

	<key>Breathiness</key>
	<real>1.0</real>

	<key>LossFactor</key>
	<real>0.5</real>

	<key>Pitch</key>
	<real>-12.0</real>

	<key>ThroatCuttoff</key>
	<real>1500.0</real>

	<key>ThroatVolume</key>
	<real>6.0</real>

	<key>ApertureScaling</key>
	<real>3.05</real>

	<key>MouthCoef</key>
	<real>5000.0</real>

	<key>NoseCoef</key>
	<real>5000.0</real>

	<key>MixOffset</key>
	<real>54.0</real>

	<key>N1</key>
	<real>1.35</real>

	<key>N2</key>
	<real>1.96</real>

	<key>N3</key>
	<real>1.91</real>

	<key>N4</key>
	<real>1.3</real>

	<key>N5</key>
	<real>0.73</real>

	<key>Tp</key>
	<real>40.0</real>

	<key>TnMin</key>
	<real>16.0</real>

	<key>TnMax</key>
	<real>32.0</real>

	<key>GpShape</key>
	<!-- Pulse / Sine -->
	<string>Pulse</string>

	<key>NoiseModulation</key>
	<true/>

	<key>SamplingRate</key>
	<!-- 22050 / 44100 -->
	<string>22050</string>

	<key>OutputChannels</key>
	<!-- Mono / Stereo -->
	<string>Mono</string>
</dict>
</plist>
